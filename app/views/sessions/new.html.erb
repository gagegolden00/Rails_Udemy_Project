<h1>login</h1>
<%= form_with(scope: :session, url: login_path, local: true) do |f| %>
  <p>
    <%= f.label :email %><br/>
    <%= f.email_field :email %>
  </p>
  <p>
    <%= f.label :password %><br/>
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.submit 'login'%>
  </p>
  <p>
    <%= link_to 'home', root_path %>
  </p>
<% end %>